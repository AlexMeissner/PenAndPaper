{
    "Serilog": {
        "Using": [ "Serilog.Sinks.Console" ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information"
            }
        },
        "WriteTo": [
            {
                "Name": "Console"
            }
        ]
    },
    "Kestrel": {
        "Endpoints": {
            "Https": {
                "Url": "https://localhost:7099"
            }
        }
    }
}
