<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:fa="http://schemas.awesome.incremented/wpf/xaml/fontawesome.sharp">

    <Style x:Key="BaseIconImage" TargetType="fa:IconImage">
    </Style>

    <Style x:Key="DefaultImage" TargetType="fa:IconImage" BasedOn="{StaticResource BaseIconImage}">
        <Setter Property="Foreground" Value="{DynamicResource Theme_Color_Font_1}"/>
        <Setter Property="Margin" Value="2"/>
    </Style>

    <Style x:Key="LoginUserImage" TargetType="fa:IconImage" BasedOn="{StaticResource BaseIconImage}">
        <Setter Property="Foreground" Value="{DynamicResource Theme_Color_Font_1}"/>
        <Setter Property="Margin" Value="2"/>
    </Style>

    <Style x:Key="LoginIconImage" TargetType="fa:IconImage" BasedOn="{StaticResource BaseIconImage}">
        <Setter Property="Foreground" Value="{DynamicResource Theme_Color_Font_1}"/>
        <Setter Property="Margin" Value="2"/>
    </Style>    

</ResourceDictionary>